{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "liveNow": false,
    "panels": [
        {
            "datasource": {
                "type": "influxdb",
                "uid": "${DS_OLLAMA_INFLUXDB}"
            },
            "description": "Диаграмма Ганта показывающая когда модели загружались и как долго это занимало",
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "hideFrom": {
                            "legend": false,
                            "tooltip": false,
                            "vis": false
                        }
                    },
                    "mappings": [
                        {
                            "options": {
                                "loading": {
                                    "color": "orange",
                                    "index": 0,
                                    "text": "Loading"
                                },
                                "ready": {
                                    "color": "transparent",
                                    "index": 1,
                                    "text": ""
                                }
                            },
                            "type": "value"
                        }
                    ]
                },
                "overrides": []
            },
            "gridPos": {
                "h": 12,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "id": 1,
            "options": {
                "mergeValues": false,
                "rowHeight": 0.9,
                "showValue": "auto",
                "tooltip": {
                    "mode": "single",
                    "sort": "none"
                }
            },
            "targets": [
                {
                    "datasource": {
                        "type": "influxdb",
                        "uid": "${DS_OLLAMA_INFLUXDB}"
                    },
                    "query": "// Получаем данные о времени загрузки\nloading_data = from(bucket: \"ollama-logs\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"ollama_sessions\")\n  |> filter(fn: (r) => r._field == \"loading_duration_seconds\")\n\n// Точки начала загрузки\nstart_points = loading_data\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _value: \"loading\",\n      model: r.model,\n      session_id: r.session_id,\n      pid: r.pid\n    }))\n\n// Точки окончания загрузки (время старта + длительность в наносекундах)\nend_points = loading_data\n  |> map(fn: (r) => ({\n      _time: time(v: int(v: r._time) + int(v: float(v: r._value) * 1000000000.0)),\n      _value: \"ready\",\n      model: r.model,\n      session_id: r.session_id,\n      pid: r.pid\n    }))\n\n// Объединяем точки начала и окончания\nunion(tables: [start_points, end_points])\n  |> sort(columns: [\"_time\"])\n  |> group(columns: [\"model\", \"session_id\", \"pid\"])",
                    "refId": "A"
                }
            ],
            "title": "Ollama Models Gantt Chart",
            "type": "state-timeline"
        }
    ],
    "refresh": "30s",
    "schemaVersion": 37,
    "style": "dark",
    "tags": [
        "ollama",
        "gantt"
    ],
    "templating": {
        "list": []
    },
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "title": "Ollama Gantt Chart",
    "uid": "ollama-gantt-simple",
    "version": 1,
    "weekStart": "",
    "__inputs": [
        {
            "name": "DS_OLLAMA_INFLUXDB",
            "label": "Ollama InfluxDB",
            "description": "",
            "type": "datasource",
            "pluginId": "influxdb",
            "pluginName": "InfluxDB"
        }
    ],
    "__requires": [
        {
            "type": "grafana",
            "id": "grafana",
            "name": "Grafana",
            "version": "9.0.0"
        },
        {
            "type": "datasource",
            "id": "influxdb",
            "name": "InfluxDB",
            "version": "1.0.0"
        },
        {
            "type": "panel",
            "id": "state-timeline",
            "name": "State timeline",
            "version": ""
        }
    ]
}